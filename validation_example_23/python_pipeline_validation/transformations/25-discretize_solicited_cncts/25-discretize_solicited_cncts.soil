!create discretize:Transformation
!create field:Field
!discretize.name:='discretize'

!create binUnknown:DiscretizeBin
!create intervalUnknown:Interval
!binUnknown.name:='binUnknown'
!binUnknown.binValue:='Unknown'
!intervalUnknown.leftMargin:=0.0
!intervalUnknown.rightMargin:=1.0
!intervalUnknown.closure:=ClosureType::closedOpen
!insert(discretize, binUnknown ) into TransfParam
!insert(binUnknown, intervalUnknown) into DiscretizeInterval
!insert(TERRITORY_0,intervalUnknown) into ContinuousInterval

